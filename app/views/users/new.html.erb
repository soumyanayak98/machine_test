<p>create new user</p>
<%= render 'shared/errors', obj: @user =%>
<%= form_for @user, method: :post do |f| =%>
    <%= f.label :name =%>
    <%= f.text_field :name =%>
    <br/>
    <%= f.label :password =%>
    <%= f.password_field :password =%>
    <br/>
    <%= f.submit =%>
    <button>
        <%= link_to "cancel", root_path =%>
    </button>
<% end %>